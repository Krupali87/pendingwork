<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_checked="false">
        <shape android:visible="true" android:shape="rectangle" android:useLevel="false">
            <size android:height="24dp" android:width="48dp"/>
            <corners android:radius="20dp"/>
            <solid android:color="#96a3ab"/>
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:visible="true" android:shape="rectangle" android:useLevel="false">
            <size android:height="24dp" android:width="48dp"/>
            <corners android:radius="20dp"/>
            <solid android:color="#32be4b"/>
            <stroke android:width="1dp" android:color="@android:color/transparent"/>
        </shape>
    </item>
</selector>
